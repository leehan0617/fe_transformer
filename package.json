{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ubuntu": "vite --host 0.0.0.0 --port 3000",
    "dev:windows": "vite --host localhost --port 3000",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:ubuntu": "vite preview --host 0.0.0.0 --port 4173",
    "preview:windows": "vite preview --host localhost --port 4173",
    "serve": "npm run build && npm run preview",
    "install:offline": "npm ci --offline",
    "setup:ubuntu": "npm run install:offline && npm run build",
    "setup:windows": "npm run install:offline && npm run build"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.2.1",
    "vite": "^5.2.10",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
